.full-screen
{
  display: none;
  position: fixed;
  overflow: hidden;
  z-index: 9999;
  height: 100%;
  width: 100%;
  top: 0px;
  left: 0px;

  .close-owl
  {
    position: absolute;
    top: 0px;
    right: 0px;
    background: #CC3939;
    width: 40px;
    height: 40px;
    color: white;
    border: none;
    z-index: 999;
    @include transition(all 0.3s ease);

    &:hover { background: black; }
  }

  #full-flex
  {
    position: relative;
    height: 300px;
    width: 100%;
    background: #111;
    display: block;
    padding: 40px 0;

    #display
    {
      position: relative;

      .item
      {
        position: relative;

        div
        {
          width: 100%;
          height: 100%;
          display: block;

          img
          {
            display: block;
            margin: 0 auto;
            width: auto;
            max-width: 900px;
            height: auto;
            max-height: 100%;
            position: relative;
          }
        }
      }
    }

    button
    {
      position: absolute;
      height: 150px;
      background: #CC3939;
      color: white;
      width: 30px;
      border: none;
      top: 50%;
      @include transition(all 0.3s ease);
      z-index: 100;
      margin-top: -75px;

      &:hover { background: black; }

      &.owl-prev-display
      {
        left: 0px;
      }

      &.owl-next-display
      {
        right: 0px;
      }
    }
  }

  .full-control
  {
    height: 150px;
    background: #000;
    width: 100%;
    display: block;
    position: absolute;
    bottom: 0px;
    left: 0px;

    span
    {
      position: absolute;
      margin: 0;
      padding: 0;
      top: -35px;
      left: 25px;
      color: white;
    }

    button
    {
      position: absolute;
      height: 150px;
      background: rgba(65, 63, 63, 0.33);
      color: white;
      width: 50px;
      border: none;
      top: 0px;
      @include transition(all 0.3s ease);

ul, ol
{
  li
  {
    line-height: 25px;
  }
}

hr
{
  width: 100%;
  height: 1px;
  margin: 40px 0;
  background-color: $border-color;
  outline: none;
  border: none;
}

blockquote
{
  max-width: 700px;
  font-style: italic;
  color: #bbbbbb;
  padding-left: 20px;
  margin-left: 20px;
}

p
{
  line-height: 24px;
}

// Content heading

.promo
{
  background: {{ settings.promo-bg-color }};
  color: {{ settings.promo-color }};
  line-height: 30px;
  display: block;
}

.content-heading
{
  margin: 60px 0 40px 0;
  padding: 15px 0;
  text-align: center;

  p
  {
    color: #999;
    margin-top: 4px;
  }
}

table
{
  border-collapse:collapse;
  border: 1px solid $border-color;
  width: 100%;

  tr:not(:first-child):hover
  {
    background: #f8fbff;
    @include transition(all 0.2s ease);
  }

  th, td
  {
    border-bottom: 1px solid $border-color;
  }

  th
  {
    padding: 18px 10px;
    text-align: left;
    border-bottom: 4px double #dbd9d4;
    background: #f3f2f1;
    color: #918e89;
  }

  td
  {
    padding: 12px 10px;
  }

  tbody, tfoot
  {
    tr:hover
    {
      background: #f8fbff !important;
    }
  }
}

.notification
{
  $notification-bg: #EBEBEB;
  $notification-color: #999595;

  background: $notification-bg;
  @include border-radius(10px);
  padding: 15px;
  position: fixed;
  top: 30px;
  width: 90%;
  right: 5%;
  color: $notification-color;
  text-align: center;
  z-index: 9999;
  @include transition(all 0.3s ease);
  visibility: hidden;
  opacity: 0;

  &.show
  {
    visibility: visible;
    opacity: 0.95;
  }

  p { cursor: default; }

  &:hover { opacity: 0.6; }
}

// General styling

.top // top css (mobile only xs-screen)
{
  border-top: 4px double $border-color;
  border-bottom: 1px double $border-color;
  margin-top: 10px;

  a
  {
    line-height: 40px;

    &:hover { text-decoration: none; }

    span
    {
      position: relative;
      vertical-align: middle;
      top: -3px;
    }
  }
}


// Side menu
#nav-menu
{
  background: $nav-bg;

  ul
  {
    li
    {
      a
      {
        color: $nav-a-color;
        text-decoration: none;
      }

      &:after
      {
        border-color: $nav-border-color;
      }
    }
    .mm-subopen:before { border-color: $nav-border-color; }
  }

  .icon-social
  {
    a
    {
      color: $nav-a-color;
      text-decoration: none;
    }
  }
}

.res-wrap { img { width: 100%; height: auto; display: block;} }

// Shopping cart
#shopping-cart
{
  $cart-bg: #fafafa;

  background: $cart-bg;
  color: $base-color;

  & > div
  {
    padding: 0;

    h2
    {
      background: #fff;
      margin: 0;
      padding: 20px 25px;
      text-align: center;
      box-shadow: 0 0 3px 0 rgba(0,0,0,0.25);

      span
      {
        text-transform: none;
        display: block;
        font-size: 13px;
        margin-top: 5px;
        font-family: $base-font;
      }
    }

    .title
    {
      border-bottom: 1px solid $border-color;
      padding-bottom: 5px;
    }

    ul
    {
      margin: 0;
      padding: 0;

      li
      {
        margin: 20px 25px;
        position: relative;

        .remove
        {
          position: absolute;
          display: block;
          right: 0px;
          top: 0px;
          width: 18px;
          font-size: 12px;
          line-height: 18px;
          @include border-radius(50%);

          background: #5E5C5C;
          color: white;
          text-align: center;
          cursor: pointer;
          @include transition(all 0.3s ease);
          z-index: 2;
          opacity: 0.5;

          &:hover { opacity: 1; }
        }

        .detail
        {
          padding-left: 25px;
        }

        .info
        {
          color: #a4afbd;
          font-size: 15px;
          margin-top: 5px;
        }

        &:after
        {
          margin-left: 0;
          margin-top: 20px;
          border-bottom: 1px solid $border-color;
        }
      }
    }

    .total
    {
      border-bottom: 1px solid $border-color;
      padding: 0 0 15px 0;
      margin: 0 25px;
      margin-top: -5px;
      font-size: 16px;
      color: #a4afbd;

      &:after
      {
        clear: both;
      }

      span
      {
        float: right;
        color: #F56946;
      }
    }

    .checkout
    {
      padding: 20px 25px;
      text-align: right;
    }
  }
}

header
{
  padding: 40px 0 0 0;
  position: relative;

  .logo
  {
    text-align: center;
  }

  & > div
  {
    text-align: center;
    margin-top: 30px;

    .search-box
    {
      position: relative;

      input
      {
        @include border-radius(40px);
        padding: 0 60px 0 20px;
        width: 100%;
      }

      .search
      {
        position: relative;
        height: 45px;
        width: 100%;
        clear: both;

        input:focus
        {
          & + span
          {
            &:before {content: "\31";}
          }
        }

        span
        {
          position: absolute;
          width: 45px;
          @include border-radius(50%);
          margin: 0;
          display: inline-block;
          vertical-align: top;
          border: 1px solid $border-color;
          line-height: 43px;
          text-align: center;
          top: 0px;
          right: 0px;
          color: #999;
          background: white;
          cursor: pointer;
        }
      }

      a
      {
        display: none;
      }
    }
  }
}


.featured
{
  margin-top: 40px;
}

.old
{
	display: inline-block;
	margin-left: 15px;
	color: #ccc;
	text-decoration: line-through;
}

.grid
{
  position: relative;
  margin: 0;
  padding: 0;
  list-style-type: none;
  left: 0px;

  .pin
  {
    width: 100%;
    position: absolute;
    visibility: hidden;

    .image
    {
      overflow: hidden;

      {% if settings.twist_scale %}

      &:hover
      {
        img
        {
          @include render(transform, rotate(-5deg) scale(1.3));
        }
      }

      {% endif %}

      img
      {
        width: 100%;
        height: auto;
        display: block;
        @include transition(all 0.4s ease);
      }
    }

    .info
    {
      margin-top: 8px;
      text-align: center;

      .price
      {
        font-size: 16px;
        margin-top: 5px;
      }

      strong
      {
        display: block;
      }

      .meta
      {
        @include render(border-radius, 8px);
        border: 1px solid $border-color;
        font-size: 0;
        height: 40px;
        width: 100%;
        text-align: left;
        overflow: hidden;
        margin-top: 8px;

        a
        {
          color: #ccc;
          width: 33.33333333%;
          font-size: 15px;
          line-height: 38px;
          display: inline-block;
          text-align: center;
          cursor: pointer;

          &:hover
          {
            background: #fcfcfc;
          }

          &:not(:last-child)
          {
            border-right: 1px solid $border-color;
          }
        }
      }
    }
  }
}

// Home page latest blogs
.latest
{
  .row
  {
    & > div
    {
      margin-bottom: 40px;
      vertical-align: top;

      h3
      {
        margin-bottom: 5px;
      }

      span
      {
        display: block;
        color: #ADB1B1;
        margin-bottom: 12px;
      }

      img
      {
        width: 100%;
        height: auto;
        display: block;
      }
    }
  }
}

.owl-item
  {
    img
    {
      width: 100%;
      height: auto;
      display: block;
      cursor: pointer;
    }
  }

footer
{
  margin-top: 50px;
  border-top: 1px solid $border-color;
  border-bottom: 1px solid $border-color;

  .row > div
  {
    padding: 30px 0 20px 0;

    h4
    {
      margin-top: 0;
    }

    ul
    {
      @include ul-reset;
      font-size: 0;

      li
      {
        width: 50%;
        display: inline-block;
        vertical-align: top;
        font-size: 13px;
        text-transform: capitalize;
        margin-bottom: 4px;

        a
        {
          display: block;
        }
      }
    }
  }
}

.end-footer
{
  color: #ADB1B1;
  font-size: 12px;
  padding: 20px 0;

  .end
  {
    text-align: center;
  }
}

.breadcrumb {
  @include ul-reset;
  margin: 30px 0;

  text-align: center;

  & > li {
    display: inline-block;

    & + li:before {
      content: "\2192";
      padding: 0 5px;
      color: #CCCFD3;
    }
  }

  a
  {
    color: $a-hover-color;

    &:hover
    {
      color: $a-color;
    }
  }

  & > .active {
    color: $base-color;
  }
}

.paginate
{
  text-align: center;
  margin-top: 50px;
  line-height: 30px;

  span:not(:last-child)
  {
    margin-right: 8px;
  }

  span:not(.prev):not(.current):not(.next)
  {
    width: 30px;
    display: inline-block;
    @include border-radius(50%);
    background: #E9E9E9;
  }
}


#product-modal
{
  .modal-dialog
  {
    background: #fff;
    box-shadow: 0 0px 15px 0px rgba(0, 0, 0, 0.2);
    padding: 30px;
  }
}


// Titles in collection page
.collection-title
{
  margin-bottom: 40px;
  position: relative;
  width: 100%;
  text-align: center;

  a
  {
    color: $heading-color;
  }

  .desc
  {
    $desc-color: #B8B8B8;
    color: $desc-color;
  }
}


// Filters in collection page
.filter
{
  position: relative;
  width: 100%;
  display: block;
  margin-top: 25px;
  text-align: left;

  .title, li > a
  {
    background: $form-elm-background;
    border: none;
    border-radius: 0;
    outline: none !important;
    width: 100%;
    font-size: 14px;
    @include transition(all 0.3s ease);
    color: $form-elm-color;
    display: block;
    width: 100%;
    position: relative;
    line-height: 45px;
    padding-left: 20px;
    cursor: pointer;

    &:hover{
      background: $form-elm-focus;
    }

    .arrow
    {
      position: absolute;
      right: 10px;
      top: 0;
      line-height: 45px;
      @include transition(all 0.3s ease);

      &.rotate
      {
        @include render(transform, rotate(-180deg));
      }
    }
  }

  ul
  {
    width: 100%;
    font-size: 0;
    display: none;
    position: absolute;
    z-index: 10;

    li
    {
      a
      {
        background: #DADADA;
        line-height: 35px;
      }
    }
  }
}

// A single product styling
.product
{
  position: relative;
  margin-top: 50px;

  .close
  {
    position: absolute;
    top: 0px;
    right: 0px;
    font-size: 15px;
    text-align: center;
    width: 25px;
    line-height: 25px;
    background: #888888;
    color: white;
    cursor: pointer;
    @include transition(all 0.3s ease);
    opacity: 1;

    &:hover
    {
      opacity: 0.7;
    }
  }



  .product-image
  {
    .product-flex
    {
      .thumbs
      {
        @include ul-reset;
        font-size: 0;
        z-index: 10;
        position: relative;

        li
        {
          display: inline-block;
          vertical-align: top;
          width: 16%;
          margin-right: 5%;
          cursor: pointer;
          margin-top: 20px;
          opacity: 0.8;
          @include transition(all 0.2s ease);

          &:hover { opacity: 1; }

          &:nth-child(5n+5)
          {
            margin-right: 0% !important;
          }
        }
      }
    }
  }

  .product-desc
  {
    margin-top: 30px;

    .product-link { margin-top: 30px; }

    .type
    {
      text-transform: uppercase;

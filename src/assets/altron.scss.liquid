html {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
}
@import url("https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css");

@mixin render($property, $value)
{
  -webkit-#{$property}: $value;
  -moz-#{$property}: $value;
  -o-#{$property}: $value;
  -ms-#{$property}: $value;
  #{$property}: $value;
}

@mixin border-box() { @include render(box-sizing, border-box); }
@mixin transition($value) { @include render(transition, $value); }
@mixin border-radius($value) { @include render(border-radius, $value); }
@mixin ul-reset() { list-style-type: none; margin: 0; padding: 0; }
input[type="text"], input[type="password"], input[type="email"], textarea, input[type="email"], input[type="number"] { -webkit-appearance: none !important; -moz-appearance: none !important; }
button { outline: none; }
.icomoon { font-family: icomoon; }

*,
*:before,
*:after
{
  @include border-box;
}

/*
  THEME SETTINGS
*/

// FONTS
//$base-font: "";
{% if settings.body-font contains 'Arial'  %}
$base-font: arial;
{% else %}
$base-font: {{ settings.body-font }};
{% endif %}

// COLOR SCHEME
$base-color: {{ settings.base-color }};
$border-color: {{ settings.border-color }};
$background-color: {{ settings.background-color }};
$heading-color: {{ settings.heading-color }};

$a-color: {{ settings.a-color }};
$a-hover-color: {{ settings.a-hover-color }}; // EC6092 FFF47B

// FORM ELEMENT COLOR SCHEME
$form-elm-background: {{ settings.form-elm-background }};
$form-elm-color: {{ settings.form-elm-color }};
$form-elm-focus: {{ settings.form-elm-focus }};

// BUTTON COLOR SCHEME
$btn-clr1: {{ settings.btn-clr1 }};
$btn-clr1-color: {{ settings.btn-clr1-color }};
$btn-clr2: {{ settings.btn-clr2 }};
$btn-clr2-color: {{ settings.btn-clr2-color }};
$btn-clr3: {{ settings.btn-clr3 }};
$btn-clr3-color: {{ settings.btn-clr3-color }};

// DESKTOP NAVIGATION COLOR SCHEME
$dsk-a-color: {{ settings.dsk-a-color }};
$dsk-a-hover-bg: {{ settings.dsk-a-hover-bg }};

$nav-a-color: {{ settings.nav-a-color }};
$nav-bg: {{ settings.nav-bg }};
$nav-border-color: {{ settings.nav-border-color }};

$footer-bg: #fbfbfa;

// MAX WIDTH OF THE CONTAINER
$max-width: 100%;


img.responsive { width: 100%; height: auto; display: block; }

h1, h2, h3, h4, h5, h6 { color: $heading-color; }

h1
{

  font: 400 40px $base-font;
  text-transform: uppercase;
}

h2
{
  font: 500 22px $base-font;
  text-transform: uppercase;
}

h3
{
  font: 500 16px $base-font;
  text-transform: uppercase;
}

h4
{
  font: 500 15px $base-font;
  text-transform: none;
}

h5
{
  font: 500 14px $base-font;
  text-transform: none;
}

h6
{
  font: 500 13px $base-font;
  text-transform: none;
}

@mixin grayscale
{
  filter: grayscale(100%);
  filter: gray; /* IE 6-9 */
  filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale");
  -webkit-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
}

body
{
  width: 100%;
  height: 100%;
  margin: 0;
  font: 400 {{settings.font-size}}px/20px $base-font;
  color: $base-color;
  position: relative;
  background: $background-color;
}

.container
{
  width: 90%;
  max-width: $max-width;
  height: auto;
  display: block;
  margin: 0 auto;
  position: relative;
  padding: 0;
}

// Styles for form elements

input[type="text"], input[type="password"], input[type="telephone"], input[type="email"], textarea, select, input[type="email"], input[type="number"]
{
  background: $form-elm-background;
  border: none;
  border-radius: 0;
  outline: none !important;
  padding: 0 14px;
  width: 100%;
  font-size: 14px;
  @include transition(all 0.1s ease);
  color: $form-elm-color;

  &:focus
  {
    background: $form-elm-focus;
  }
}

input[type="text"], input[type="email"], input[type="number"], input[type="password"], input[type="telephone"], select
{
  height: 45px;
}

label
{
  margin-bottom: 6px;
  display: block;
}

textarea
{
  padding-top: 10px;
  padding-bottom: 10px;
  height: 150px;
}

.form-group
{
  margin-bottom: 15px;

  &.or
  {
    button
    {
      margin-right: 15px;
    }

    a
    {
      margin-left: 5px;
    }
  }
}

// Icomoon settings

[class*="icon-"]
{
  font-size: 12px;
  &.fs10 { font-size: 10px; }
  &.fs15 { font-size: 15px; }
  &.fs25 { font-size: 25px; }
  &.fs30 { font-size: 30px; }

  &:hover{
    text-decoration: none !important;
  }
}

a
{
  text-decoration: none;
  @include transition(all 0.3s ease);
  color: $a-color;
  outline: none;

  &:hover
  {
    color: $a-hover-color;
    text-decoration: underline;
  }

  &.underline
  {
    text-decoration: underline;
  }
}

// Button styles

.button
{
  line-height: 45px;
  padding: 0 20px;
  font-size: 14px;
  text-align: center;
  display: inline-block;
  text-decoration: none;
  @include transition(all 0.2s ease);
  position: relative;
  border: 0;

  &:active
  {
    top: 1px;
  }

  &:hover
  {
    opacity: 0.8;
    text-decoration: none;
  }

  &.clr1
  {
    background: $btn-clr1;
    color: $btn-clr1-color;
  }

  &.clr2
  {
    background: $btn-clr2;
    color: $btn-clr2-color;
  }

  &.clr3
  {
    background: none;
    border: 2px solid $btn-clr3;
    color: $btn-clr3-color;
    padding: 0 14px;
  }

  &.loading
    {
      background: #DDD;
      color: white;
      cursor: default;
      opacity: 0.6;
      &:hover { opacity: 0.6; }
    }
}

.pseudo-select
{
  position: relative;

  .selected
  {
    background: $form-elm-background;
    border: none;
    border-radius: 0;
    outline: none !important;
    padding: 0 14px;
    width: 100%;
    font-size: 14px;
    @include transition(all 0.1s ease);
    color: $form-elm-color;
    line-height: 45px;
    cursor: pointer;

    &:focus
    {
      background: $form-elm-focus;
    }

    [class*="icon-"]
    {
      position: absolute;
      right: 10px;
      top: 0px;
      line-height: 45px;
      display: block;
      font-size: 15px;
      @include transition(all 0.2s ease);
    }
  }

  ul
  {
    margin: 0;
    padding: 0;
    list-style-type: none;
    max-height: 200px;
    overflow-x: hidden;
    overflow-y: auto;
    border: 1px solid $border-color;
    padding: 8px;
    position: absolute;
    top: 45px;
    left: 0px;
    width: 100%;
    background: #fff;
    display: none;
    z-index: 10;
    @include transition(all 0.3s ease);

    &.show
    {
      display: block;
    }

    li
    {
      color: #25282a;
      background: #ffffff;
      @include transition(all 0.06s ease);
      line-height: 30px;
      padding-left: 8px;
      cursor: pointer;

      &:hover
      {
        color: #ffffff;
        background: #25282a;
      }

      a
      {
        color: #25282a;
        background: #ffffff;
        @include transition(all 0.06s ease);
        display: block;

        &:hover
        {
          color: #ffffff;
          background: #25282a;
        }
      }
    }
  }
}


